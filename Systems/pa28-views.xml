<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

	<view n="0">
			<name>Pilot View</name>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.24</x-offset-m>	
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<pitch-offset-deg>-14.5455</pitch-offset-deg>
				<default-field-of-view-deg type="double">82</default-field-of-view-deg>
			</config>
		</view>
		<view n="1">
			<name>Helicopter View</name>
			<config>
				<z-offset-m>-20</z-offset-m>

				<y-offset-m archive="y">0.2</y-offset-m>
				<heading-offset-deg>180</heading-offset-deg>
<z-offset-m>-25</z-offset-m>
			</config>
		</view>

		<view n="2">
			<name>Chase View</name>
			<config>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
			<pitch-offset-deg>-5</pitch-offset-deg>
			</config>
		</view>
		<view n="5">
			<name>Chase without Yaw</name>
			<config>
				<z-offset-m>-10</z-offset-m>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
				<target-x-offset-m archive="y" type="double">-1</target-x-offset-m>
<heading-offset-deg>2</heading-offset-deg>

			</config>
		</view>
		<view n="7">
			<config>
				<name>Model View</name>
				<target-z-offset-m archive="y" type="double">3.0</target-z-offset-m>
			</config>
		</view>
		<view n="100">
			<name>Copilot View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.24</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<pitch-offset-deg>-15</pitch-offset-deg>
				<default-field-of-view-deg type="double">82</default-field-of-view-deg>
			</config>
		</view>
		<view n="101">
			<name>Cockpit View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-0.10</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<pitch-offset-deg>-15</pitch-offset-deg>
				<default-field-of-view-deg type="double">82</default-field-of-view-deg>
			</config>
		</view>
		<view n="102">
			<name>IFR</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-0.10</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.8</z-offset-m>
				<pitch-offset-deg>-21</pitch-offset-deg>
				<default-field-of-view-deg type="double">55</default-field-of-view-deg>
			</config>
		</view>	
		
		<view n="103">
			<name>Rear</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-0.10</x-offset-m>
				<y-offset-m archive="y">0.5</y-offset-m>
				<z-offset-m archive="y">2.6</z-offset-m>
				<heading-offset-deg>195</heading-offset-deg>
				<pitch-offset-deg>-33</pitch-offset-deg>
				<roll-offset-deg>-3</roll-offset-deg>
				<default-field-of-view-deg type="double">114</default-field-of-view-deg>
			</config>
		</view>
				
	<view n="122">
		<name>R Wing</name>
		<type>lookfrom</type>
		<internal type="bool" archive="y">false</internal>
		<config>
			<from-model type="bool">true</from-model>
			<x-offset-m archive="y">1</x-offset-m>
			<y-offset-m archive="y">1</y-offset-m>
			<z-offset-m archive="y">4.9</z-offset-m>
			<pitch-offset-deg>-7</pitch-offset-deg>
			<heading-offset-deg>350</heading-offset-deg>
			<default-field-of-view-deg type="double">125</default-field-of-view-deg>
		</config>
	</view>
	<view n="123">
		<name>L Wing</name>
		<type>lookfrom</type>
		<internal type="bool" archive="y">false</internal>
		<config>
			<from-model type="bool">true</from-model>
			<x-offset-m archive="y">-1.5</x-offset-m>
			<y-offset-m archive="y">0.76</y-offset-m>
			<z-offset-m archive="y">5.24</z-offset-m>
			<pitch-offset-deg>-7</pitch-offset-deg>
			<heading-offset-deg>9</heading-offset-deg>
			<default-field-of-view-deg type="double">125</default-field-of-view-deg>
		</config>
	</view>		
	
		<view n="121">
        <name>Tail B</name>
        <enabled type="bool">true</enabled>
        <type>lookfrom</type>
        <internal type="bool">false</internal>
        <config>
            <from-model type="bool">true</from-model>
            <from-model-idx type="int">0</from-model-idx>
            <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
            <default-field-of-view-deg type="double">80</default-field-of-view-deg>
            <default-pitch-deg type="double">-15</default-pitch-deg>
            <default-heading-deg type="double">0</default-heading-deg>
            <front-direction-deg type="double">0</front-direction-deg>
            <front-left-direction-deg type="double">45</front-left-direction-deg>
            <left-direction-deg type="double">90</left-direction-deg>
            <back-left-direction-deg type="double">135</back-left-direction-deg>
            <back-direction-deg type="double">180</back-direction-deg>
            <back-right-direction-deg type="double">225</back-right-direction-deg>
            <right-direction-deg type="double">270</right-direction-deg>
            <front-right-direction-deg type="double">315</front-right-direction-deg>
            <dynamic-view type="bool">true</dynamic-view>
            <x-offset-m type="double"> 0</x-offset-m>
            <y-offset-m type="double"> 3</y-offset-m>
            <z-offset-m type="double"> 11.5</z-offset-m>
            <pitch-offset-deg>-15</pitch-offset-deg>
		 <heading-offset-deg>180</heading-offset-deg>
 
        </config>
    </view>


 
</PropertyList>
