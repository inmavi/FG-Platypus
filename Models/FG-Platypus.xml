	</animation>

	<animation>
		<type>material</type>
		<object-name>prop disc front</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>frontdisc</texture-prop>
		<texture>Liveries/propblur.png</texture>
	</animation>

	<!-- Nasal code -->
	<nasal>
		<load>
		var livery_update = aircraft.livery_update.new("Aircraft/FG-Platypus/Models/Liveries");
		</load>
		<unload>
		livery_update.stop();
		</unload>
	</nasal>

