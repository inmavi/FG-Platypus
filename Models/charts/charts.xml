<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>charts.ac</path>
  
  <animation>
    <object-name>carte</object-name>
  </animation>

 <animation>
    <type>pick</type>
    <type>material</type>
    <object-name>carte</object-name>
    <action>
    <button>0</button>
    <repeatable>false</repeatable>
            <binding>
				<command>dialog-show</command>
				<dialog-name>map</dialog-name>
			</binding>
		<binding>
		<command>property-assign</command>
		<property>gui/map/transparent</property>
		<value>1</value>  
  </binding>
	<binding>
		<command>property-assign</command>
		<property>gui/map/modal</property>
		<value>false</value>  
  </binding>

  <binding>
		<command>dialog-close</command>
		<dialog-name>map</dialog-name>
  </binding>

  <binding>
		<command>dialog-show</command>
		<dialog-name>map</dialog-name>
  </binding>
	
     </action>

      <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>chart</tooltip-id>
                <label>Show Map</label>
            </binding>
        </hovered>
	
  </animation>
</PropertyList>
