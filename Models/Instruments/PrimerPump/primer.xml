<?xml version="1.0"?>

<PropertyList>

 <path>primer.ac</path>

 <animation>
  <type>material</type>
  <object-name>PrimeFace</object-name>
  <object-name>PrimeShaft</object-name>
  <object-name>PrimeLock</object-name>
  <emission>
    <red>0.1</red>
    <green>0.02</green>
    <blue>0.00001</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>

 <animation> 
   <type>translate</type>
   <object-name>PrimeFace</object-name>
   <object-name>PrimeShaft</object-name>
   <property>controls/engines/engine/primer-pump</property>
   <factor>-0.035</factor>
   <axis> 
     <x>-1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>PrimeFace</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine/primer-pump</property>
      </binding>
    </action>
  </animation>

</PropertyList>



