<?xml version="1.0"?>
<PropertyList>

  <!--
  Marker Bracon Lights
  -->
  <params>
    <outer>instrumentation/marker-beacon/outer</outer>
    <middle>instrumentation/marker-beacon/middle</middle>
    <inner>instrumentation/marker-beacon/inner</inner>
  </params>

  <path>MkrBcn.ac</path>

  <animation>
    <type>material</type>
    <object-name>O</object-name>
    <emission>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
      <factor-prop alias="../../../params/outer"/>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>M</object-name>
    <emission>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
      <factor-prop alias="../../../params/middle"/>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>A</object-name>
    <emission>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
      <factor-prop alias="../../../params/inner"/>
    </emission>
  </animation>
  
   <animation>
    <type>pick</type>
    <object-name>O</object-name>
    <object-name>M</object-name>
    <object-name>A</object-name>
    <action>
    <button>0</button>
    <repeatable>false</repeatable>
        

  <binding>
		<command>dialog-show</command>
		<dialog-name>check1</dialog-name>
  </binding>
	
     </action>

      <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>ch1</tooltip-id>
                <label>Show CheckLists</label>
            </binding>
        </hovered>
	

  </animation>


</PropertyList>




