    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>pt-ann</object-name>
    <name>Pitch trim annunciator</name>
    <condition>
      <and>
      <equals>
        <property>/it-autoflight/output/ap</property>
        <value>1</value>
      </equals>
      <equals>
        <property>/it-autoflight/kap140/pt</property>
