  </animation>
  <animation>
    <type>select</type>
    <object-name>alt-arm-ann</object-name>
    <name>ALT-ARM Anunciator</name>
    <condition>
	  <or>
      <and>
      <equals>
        <property>/it-autoflight/output/ap</property>
