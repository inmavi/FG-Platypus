        <equals>
          <property>/it-autoflight/kap140/display-mode</property>
          <value>VS</value>
        </equals>
        <greater-than-equals>
          <property>/it-autoflight/input/vs</property>
          <value>0</value>
        </greater-than-equals>
      </and>
    </condition>
