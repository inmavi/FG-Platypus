<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <path>Aircraft/FG-Platypus/Models/Instruments/Svn/svn.ac</path>


  <params>
    <value>instrumentation/adf/model</value>
  </params>

  <offsets>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>0</z-m>
  </offsets>

  <animation>
    <object-name>Lever</object-name>
    <type>pick</type>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-cycle</command>
        <property alias="../../../../params/value"/>
        <value type="string">ki228</value>
        <value type="string">ki227</value>
      </binding>
       </action>
      <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>ToogleVOR_ADF</tooltip-id>
                <label>Toogle VOR in RI/RMI</label>
            </binding>
        </hovered>

  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Lever</object-name>
    <property alias="../../params/value"/>
        <factor>32</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>

  </animation>

</PropertyList>
















































