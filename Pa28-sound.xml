<?xml version="1.0"?>
 
<!-- New Sounds By Gabriel Hernandez(YV3399) -->

<PropertyList>

 <fx>

    <!-- Out Of Fuel -->
    <oof>
      <name>Alarme-carburant</name>
      <mode>looped</mode>
      <path>PNavSounds/alarm.wav</path>
      <condition>
          <equals>
            <property>consumables/fuel/total-fuel-lbs</property>
            <value>0</value>
          </equals>
      </condition>
	      <volume>
        <factor>1.1</factor>
      </volume>
      <reference-dist>5</reference-dist>
      <max-dist>1000</max-dist>      
    </oof>

    <!-- Low Of Fuel -->
    <oof>
      <name>Alarme-Low</name>
      <mode>once</mode>
      <path>PNavSounds/alarme_fuel.wav</path>
      <condition>          
            <property>systems/fuel/low-fuel</property>
      </condition>
	      <volume>
        <factor>1.1</factor>
      </volume>
      <reference-dist>80</reference-dist>
      <max-dist>1000</max-dist>      
    </oof>

	
	<flap>
            <name>flaps</name>
            <mode>in-transit</mode>
            <path>PNavSounds/flaps.wav</path>
            <property>surface-positions/flap-pos-norm</property>
            <position>
                <x>0.5</x>
                <y>0.0</y>
                <z>0.5</z>
            </position>
            <volume>
			<factor>1.1</factor>

		<reference-dist>70</reference-dist>
		<max-dist>1000</max-dist>
		
			<offset> 1.1 </offset>
		</volume>

            
        </flap>



<!-- Running -->

   <engine>
      <name>engine</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <property>engines/engine[0]/running</property>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <pitch>
	<property>engines/engine[0]/rpm</property>
	<factor>0.0004</factor>
	<min>0.3</min>
	<max>1000.0</max>
	<offset>0.1</offset>
      </pitch>
      <volume>
	<property>sim/current-view/internal</property>
	<factor>-0.25</factor>
	<offset>0.8</offset>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>10000.0</max-dist>
    </engine>

<!-- Crank -->

    <crank>
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>engines/engine/running</property>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <volume>
        <offset>0.3</offset>
      </volume>
    </crank>

<!-- Cranking -->

    <engine>
      <name>cranking</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <condition>
        <and>
          <property>engines/engine[0]/cranking</property>
        </and>
      </condition>
      <volume>
	<property>sim/current-view/internal</property>
	<factor>-0.2</factor>
	<offset>0.9</offset>
      </volume>
      <pitch>
        <property>engines/engine/rpm</property>
        <factor>0.002</factor>
        <min>0.2</min>
        <max>1.2</max>
        <offset>0.04</offset>
      </pitch>
      <reference-dist>10.0</reference-dist>
      <max-dist>10000.0</max-dist>
    </engine>

<!-- Propeller -->

    <propeller>
      <name>propeller</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <position>
        <x>-3.0</x>
        <y> 0.0</y>
        <z> 0.0</z>
      </position>
      <condition>
        <greater-than>
          <property>engines/engine/rpm</property>
          <value>10</value>
        </greater-than>
      </condition>
      <volume>
        <property>engines/engine/rpm</property>
        <factor>0.0001</factor>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>engines/engine/rpm</property>
        <factor>0.0004</factor>
        <min>0.0</min>
        <max>2.0</max>
        <offset>0.01</offset>
      </pitch>
    </propeller>

<!-- Wind -->

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.0005</factor>
	<min>0.01</min>
	<max>0.4</max>
      </volume>
    </wind>

<!-- Rattle -->

    <rattle>
      <name>rattle</name>
      <mode>looped</mode>
      <path>Sounds/rattleroll.wav</path>
      <condition>
        <and>
          <or>
            <property>gear/gear[0]/wow</property>
            <property>gear/gear[1]/wow</property>
            <property>gear/gear[2]/wow</property>
          </or>
          <property>sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.005</factor>
	<min>0.0</min>
	<max>0.25</max>
      </volume>
    </rattle>

<!-- Touchdown -->

    <touchdown>
      <name>nose</name>
      <mode>once</mode>
      <path>Sounds/squeal.wav</path>
      <property>gear/gear[0]/wow</property>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.02</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
      <volume>
	<property>velocities/speed-down-fps</property>
	<factor>0.1</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
    </touchdown>

    <touchdown>
      <name>left</name>
      <mode>once</mode>
      <path>Sounds/squeal.wav</path>
      <property>gear/gear[0]/wow</property>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.02</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
      <volume>
	<property>velocities/speed-down-fps</property>
	<factor>0.17</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
    </touchdown>

    <touchdown>
      <name>right</name>
      <mode>once</mode>
      <path>Sounds/squeal.wav</path>
      <property>gear/gear[2]/wow</property>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.02</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
      <volume>
	<property>velocities/speed-down-fps</property>
	<factor>0.17</factor>
	<min>0.01</min>
	<max>0.5</max>
      </volume>
    </touchdown>

<!-- Stall -->

    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>PNavSounds/stall.wav</path>
      <condition>
        <and>
          <property>sim/current-view/internal</property>
          <greater-than>
	    <property>velocities/airspeed-kt</property>
            <value>10</value>
          </greater-than>
          <not>
            <property>gear/gear[0]/wow</property>
            <property>gear/gear[1]/wow</property>
            <property>gear/gear[2]/wow</property>
          </not>
        </and>
      </condition>
      <volume>
        <property>velocities/airspeed-kt</property>
        <offset>-0.2</offset>
        <factor>0.03</factor>
        <min>0.0</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>fdm/jsbsim/aero/alpha-deg</property>
        <offset>-0.6</offset>
        <factor>0.08</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>50.0</max-dist>
    </stall>

<!-- Rain -->

    <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>Sounds/rain.wav</path>
      <volume>
        <property>/environment/rain-norm</property>
        <factor>1.2</factor>
        <max>0.8</max>
      </volume>
      <volume>
	<property>sim/current-view/internal</property>
        <factor>0.3</factor>
        <min>0.5</min>
      </volume>
      <pitch>
	<property>sim/current-view/internal</property>
        <factor>0.5</factor>
      </pitch>
    </rain>

<!-- Switches -->
		
		<switch>
			<name>Switch1</name>
			<mode>once</mode>
			<path>Aircraft/FG-Platypus/PNavSounds/switch1.wav</path>
			<condition>
				<property>/sim/sounde/switch1</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Switch2</name>
			<mode>once</mode>
			<path>Aircraft/FG-Platypus/PNavSounds/switch2.wav</path>
			<condition>
				<property>/sim/sounde/switch2</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Switch3</name>
			<mode>once</mode>
			<path>Aircraft/FG-Platypus/PNavSounds/switch3.wav</path>
			<condition>
				<property>/sim/sounde/switch3</property>
			</condition>
			<volume>
				<factor>0.75</factor>
			</volume>
		</switch>
		
		<switch>
			<name>Knob</name>
			<mode>once</mode>
			<path>Aircraft/FG-Platypus/PNavSounds/knob.wav</path>
			<condition>
				<property>/sim/sounde/knb1</property>
			</condition>
			<volume>
				<factor>0.60</factor>
			</volume>
		</switch>


		<!-- Door Open-->

          <dooropen>
		<name>door open</name>
		<mode>once</mode>
		<path>PNavSounds/lidcreak.wav</path>
		<condition>
		<property>fdm/jsbsim/extra/door-main-cmd</property>
	</condition>
	<volume>
			<factor>1.1</factor>

		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
		
			<offset> 1.1 </offset>
		</volume>
	</dooropen>

		<!-- Door Close-->

      <doorclose>
		<name>door close</name>
		<mode>once</mode>
		<path>PNavSounds/door_close.wav</path>
		<condition>
			<not>
		<property>fdm/jsbsim/extra/door-main-cmd</property>
			</not>
	</condition>
	<volume>
		<reference-dist>100</reference-dist>
		<max-dist>1000</max-dist>
		
			<offset> 1.1 </offset>
		</volume>
	</doorclose>

		<apoff>
			<name>apoff</name>
			<path>PNavSounds/apoff.wav</path>
			<condition>
			<not>
				<property>/it-autoflight/input/ap</property>			
			</not>
			</condition>
			<volume>
				<factor>40.0</factor>
			</volume>
	</apoff>


		<!-- RadoAltimeter-->

         <alarmalt>
		<name>alarmalt</name>
		<mode>once</mode>
		<path>PNavSounds/alarme_fuel.wav</path>
			
            <condition>
				<less-than-equals>
                    		<property>position/altitude-agl-ft</property>
                    		<property>instrumentation/radar-altimeter/decision-height</property>

                  	</less-than-equals>
            </condition>

		<reference-dist>2</reference-dist>
		<max-dist>5</max-dist>
		<volume>
			<offset>1.1</offset>
		</volume>
	</alarmalt>

 <!--                  Toggle Switches                  -->     
 
      <click> 
        <name>Battery on</name>
        <path>Sounds/click.wav</path>
        <condition> 
          <property>controls/electric/battery-switch</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>Battery off</name>
        <path>Sounds/click.wav</path>
         <condition> 
          <not> 
            <property>controls/electric/battery-switch</property>
          </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

      <click> 
        <name>Alternator on</name>
        <path>Sounds/click.wav</path>
        <condition> 
          <property>controls/electric/alternator-switch</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>Alternator off</name>
        <path>Sounds/click.wav</path>
         <condition> 
          <not> 
            <property>controls/electric/alternator-switch</property>
          </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

      <click> 
        <name>Fuel Pump on</name>
        <path>Sounds/click.wav</path>
        <condition> 
          <property>controls/engines/engine/fuel-pump</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>Fuel Pump off</name>
        <path>Sounds/click.wav</path>
        <condition> 
          <not> 
            <property>controls/engines/engine/fuel-pump</property>
          </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>

      <click> 
        <name>Nav Light On</name>
        <path>Sounds/click.wav</path>
        <condition>
         <property>controls/switches/nav-lights</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      <click> 
        <name>Nav Light Off</name>
        <path>Sounds/click.wav</path>
        <condition>
         <not>
          <property>controls/switches/nav-lights</property>
         </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
 
      <click>
        <name>Landing Light On</name>
        <path>Sounds/click.wav</path>
        <condition>
          <property>controls/switches/landing-light</property>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>
      
      <click>
        <name>Landing Light Off</name>
        <path>Sounds/click.wav</path>
        <condition>
          <not>
            <property>controls/switches/landing-light</property>
          </not>
        </condition>
        <volume>
          <factor>1.0</factor>
        </volume>
      </click>

	<click> 
        <name>Pitot Heat On</name>
        <path>Sounds/click.wav</path>
        <condition>
          <property>controls/anti-ice/pitot-heat</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
 
      <click> 
        <name>Pitot Heat Off</name>
        <path>Sounds/click.wav</path>
        <condition>
         <not>
           <property>controls/anti-ice/pitot-heat</property>
         </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
      
      <click> 
        <name>Panel Lights On</name>
        <path>Sounds/click.wav</path>
        <condition>
          <property>controls/switches/panel-lights</property>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>
 
      <click> 
        <name>Panel Lights Off</name>
        <path>Sounds/click.wav</path>
        <condition>
         <not>
           <property>controls/switches/panel-lights</property>
         </not>
        </condition>
        <volume> 
          <factor>1.0</factor>
        </volume>
      </click>


      </fx>
 
</PropertyList>
