<?xml version="1.0"?>

<PropertyList>
	<name>FGNav</name>
	<enable3d>false</enable3d>
 <label>
		<name>NavHud</name>			
		<x>186</x>
		<y>230</y>
		<halign>left</halign>
		<format>NavHud</format>
  </label>

<label>
		<name>rwele</name>
		<condition>
         		<equals>
            			<property>/gear/gear/wow</property>
					<value>1</value>
         		</equals>
      		</condition>

		<x>-420</x>
		<y>210</y>
		<halign>left</halign>
		<format>Rwy Elev: %d ft</format>
		<input>
			<property>/position/altitude-ft</property>
		</input>
</label>

<label>
        	<name>Nav1</name>
      		<condition>
         		<equals>
            			<property>/options/nav-source</property>
					<value>1</value>
         		</equals>
      		</condition>      		
        		<x>-420</x>
        		<y>200</y>
        		<format>Nav1 Mode active</format>
        		<halign>left</halign>
   	</label>
			
		<label>
        	<name>GPS/Route Manager</name>
      		<condition>
         		<equals>
            			<property>/options/nav-source</property>
					<value>2</value>
         		</equals>
      		</condition>      		
        		<x>-420</x>
        		<y>200</y>
        		<format>GPS/Route Manager active</format>
        		<halign>left</halign>
   	</label>

		<label>
        	<name>Nav2</name>
      		<condition>
         		<equals>
            			<property>/options/nav-source</property>
					<value>3</value>
         		</equals>
      		</condition>      		
        		<x>-420</x>
        		<y>200</y>
        		<format>Nav2 Mode active</format>
        		<halign>left</halign>
   	</label>

<label>
        	<name>GSR</name>
      		<condition>
         		<not-equals>
            			<property>/instrumentation/nav[1]/gs-in-range</property>
            			<value>0</value>
         		</not-equals>
      		</condition>
      		<input>
         		<property>/instrumentation/nav[1]/gs-in-range</property>
      		</input>
        		<x>240</x>
        		<y>230</y>
        		<format>GS In Range (Nav2)</format>
        		<halign>left</halign>
   	</label>

<label>
        	<name>GSR1</name>
      		<condition>
         		<not-equals>
            			<property>/instrumentation/nav[0]/gs-in-range</property>
            			<value>0</value>
         		</not-equals>
      		</condition>
      		<input>
         		<property>/instrumentation/nav[0]/gs-in-range</property>
      		</input>
        		<x>240</x>
        		<y>210</y>
        		<format>GS In Range (Nav1)</format>
        		<halign>left</halign>
   	</label>

<label>
		<name>dmem</name>
		<condition>
         		<equals>
            			<property>/instrumentation/dme/in-range</property>
					<value>true</value>
         		</equals>
      		</condition>
		<x>-420</x>
		<y>180</y>
		<halign>left</halign>
		<format>DME: %1.1f nm</format>
		<input>
			<property>/instrumentation/dme/indicated-distance-nm</property>
		</input>
</label>

<label>
		<name>dmet</name>
		<condition>
         		<equals>
            			<property>/instrumentation/dme/in-range</property>
					<value>true</value>
         		</equals>
      		</condition>
		<x>-360</x>
		<y>180</y>
		<halign>left</halign>
		<format>...%1.1f min</format>
		<input>
			<property>/instrumentation/dme/indicated-time-min</property>
		</input>
</label>
	
	<import>
		<path>Huds/Sets/gps.xml</path>
		<x-offset>-420</x-offset>
		<y-offset>160</y-offset>
	</import>	

 <label>
		<name>Wind</name>
		<x>-420</x>
		<y>-160</y>
		<halign>left</halign>
		<format>Wind Speed: %1.2f knots</format>
		<input>
			<property>/environment/wind-speed-kt</property>
		</input>
</label>

 <label>
		<name>From</name>
		<x>-420</x>
		<y>-150</y>
		<halign>left</halign>
		<format>Wind Hdg:    %d deg</format>
		<input>
			<property>/environment/config/interpolated/wind-from-heading-deg</property>
		</input>
</label>

<label>
		<name>gs</name>
		<condition>
         		<equals>
            			<property>/sim/failure-manager/instrumentation/airspeed-indicator/serviceable</property>
					<value>true</value>
         		</equals>
      		</condition>
		<x>-420</x>
		<y>-60</y>
		<halign>left</halign>
		<format>Grd-Speed:     %d kts</format>
		<input>
			<property>/velocities/groundspeed-kt</property>
		</input>
	</label>
	
		<label>
		<name>rpm</name>
		<x>-420</x>
		<y>-70</y>
		<halign>left</halign>
		<format>Eng Revs:    %d rpm</format>
		<input>
			<property>/engines/engine/rpm</property>
		</input>
	</label>
	
	<label>
		<name>vs</name>
		<x>-420</x>
		<y>-80</y>
		<halign>left</halign>
		<format>V - Speed:     %d fpm</format>
		<input>
			<property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
		</input>
	</label>

  <label>
		<name>ADF/NDB</name>
		<x>-420</x>
		<y>-100</y>
		<halign>left</halign>
		<format>Adf/Ndb:   %s</format>
		<input>
			<property>/instrumentation/adf/ident</property>
		</input>
</label>

 <label>
		<name>Nav1-Id</name>
		<x>-420</x>
		<y>-110</y>
		<halign>left</halign>
		<format>Nav1-ID:   %s</format>
		<input>
			<property>/instrumentation/nav/nav-id</property>
		</input>
</label>

 <label>
		<name>Nav2-Id</name>
		<x>-420</x>
		<y>-120</y>
		<halign>left</halign>
		<format>Nav2-ID:   %s</format>
		<input>
			<property>/instrumentation/nav[1]/nav-id</property>
		</input>
</label>



 <label>
		<name>maghead</name>
		<x>-420</x>
		<y>-130</y>
		<halign>left</halign>
		<format>Mag Hdg:  %1.1f deg</format>
		<input>
   	<property>/orientation/heading-magnetic-deg</property>
		</input>
</label>

 <label>
		<name>magvar</name>
		<x>-420</x>
		<y>-140</y>
		<halign>left</halign>
		<format>Mag Var:  %1.1f deg</format>
		<input>
   	<property>/environment/magnetic-variation-deg</property>
		</input>
</label>

<label>
		<name>Fuell</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-190</y>
		<halign>left</halign>
		<format>Fuel-L: %2.1f gls</format>
		<input>
			<property>/consumables/fuel/tank/level-gal_us</property>
		</input>
	</label>

<label>
		<name>FFlow</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-200</y>
		<halign>left</halign>
		<format>FFlow: %2.1f gph</format>
		<input>
			<property>/engines/engine/fuel-flow-gph</property>
		</input>
	</label>

<label>
        	<name>Airtime</name>     		
      		
        		<x>-420</x>
        		<y>-90</y>
        		<format>Airborne: %s</format>
        		<halign>left</halign>
			<input>
			<property>/instrumentation/davtron803/flight-time</property>
			</input>
   	</label>

<tape>
			<condition>
         		<equals>
            			<property>/sim/failure-manager/instrumentation/altimeter/serviceable</property>
					<value>true</value>
         		</equals>
      		</condition>
		<name>RadioAltimeter</name>
		<x>-420</x>
		<y>-40</y>
		<width>25</width>
		<height>150</height>
		<option>left</option>
		<option>vertical</option>
		<major-divisions>25</major-divisions>
		<minor-divisions>5</minor-divisions>
		<display-span>200.0</display-span>
		<tick-bottom>true</tick-bottom>
		<tick-top>true</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>true</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<tick-type>line</tick-type>
		<tick-length>variable</tick-length>

		<input>
			<property>/position/gear-agl-ft</property>
			<min>0.0</min>
		</input>

		<condition>
			<less-than>
				<property>/position/gear-agl-ft</property>
				<value>1000000</value>
			</less-than>
		</condition>
	</tape>

	<tape>
		<condition>
         		<equals>
            			<property>/sim/failure-manager/instrumentation/altimeter/serviceable</property>
					<value>true</value>
         		</equals>
      		</condition>
			  <condition>
   <less-than>
    <property>/position/altitude-agl-ft</property>
    <value>500.0</value>
   </less-than>
  </condition>
		<name>AMSL</name>
		<x>-380</x>
		<y>-40</y>
		<width>30</width>
		<height>150</height>
		<option>left</option>
		<option>vertical</option>
		<major-divisions>100</major-divisions>
		<minor-divisions>20</minor-divisions>
		<display-span>200.0</display-span>
		<tick-bottom>true</tick-bottom>
		<tick-top>true</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>false</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>true</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<tick-type>line</tick-type>
		<tick-length>variable</tick-length>

		<input>
					<property>instrumentation/altimeter/indicated-altitude-ft</property>

		</input>
	</tape>

  <label>
		<name>FuelR</name>
		<width>2</width>
		<height>2</height>
		<x>-330</x>
		<y>-190</y>
		<halign>left</halign>
		<format>Fuel-R: %2.1f gls</format>
		<input>
			<property>/consumables/fuel/tank[1]/level-gal_us</property>
		</input>
	</label>

	<label>
		<name>FuelR</name>
		<width>2</width>
		<height>2</height>
		<x>-330</x>
		<y>-200</y>
		<halign>left</halign>
		<format>Fuel used: %2.3f gal US</format>
		<input>
				<property>consumables/fuel/total-fuel-used-gal_us</property>
		</input>
	</label>
	
   <label>
		<name>odom</name>
		<width>2</width>
		<height>2</height>
		<x>-420</x>
		<y>-180</y>
		<halign>left</halign>
		<format>Flown: %1.1f nm</format>
		<input>
			<property>/instrumentation/gps/trip-odometer</property>
		</input>
	</label>

	<label>
        	<name>ete</name> 
		<condition>
         		<equals>
            			<property>/options/nav-source</property>
					<value>2</value>
         		</equals>
      		</condition>   		
      		
        		<x>-420</x>
        		<y>120</y>
        		<format>ETE Dest: %s</format>
        		<halign>left</halign>
			<input>
			<property>/Aircraft/PA28-Warrior/Dialogs/etwatch/display</property>
			</input>
   	</label>

	<label>
        	<name>eta</name> 
		<condition>
         		<equals>
            			<property>/options/nav-source</property>
					<value>2</value>
         		</equals>
      		</condition>   		
      		
        		<x>-420</x>
        		<y>130</y>
        		<format>ETA: %s hh:mm</format>
        		<halign>left</halign>
			<input>
			<property>/Aircraft/PA28-Warrior/Dialogs/eta/display</property>
			</input>
   	</label>

   <label>
		<name>rem</name>
		<width>2</width>
		<height>2</height>
		<x>-330</x>
		<y>-180</y>
		<halign>left</halign>
		<format>Rem: %1.1f nm</format>
		<input>
			<property>/autopilot/route-manager/distance-remaining-nm</property>
		</input>
	</label>

        <tape>
		<condition>			
         		<not-equals>
            		<property>/systems/nhtp</property>
				<value>1</value>
         		</not-equals>			
      	</condition>     		

		<name>Gyrocompass</name>
		<x>-100</x>
		<y>190</y>
		<width>200</width>
		<height>28</height>

		<input>
			<property>/orientation/heading-magnetic-deg</property>
		</input>
		<display-span>25.0</display-span>
		<modulo>360</modulo>
		<option>top</option>
		<major-divisions>5</major-divisions>
		<minor-divisions>1</minor-divisions>
		<tick-bottom>false</tick-bottom>
		<tick-top>false</tick-top>
		<tick-right>false</tick-right>
		<tick-left>false</tick-left>
		<cap-bottom>true</cap-bottom>
		<cap-top>false</cap-top>
		<cap-right>false</cap-right>
		<cap-left>false</cap-left>
		<marker-offset>0.0</marker-offset>
		<enable-pointer>true</enable-pointer>
		<pointer-type>fixed</pointer-type>
		<tick-type>line</tick-type>
		<tick-length>variable</tick-length>
	</tape>
 	
</PropertyList>
